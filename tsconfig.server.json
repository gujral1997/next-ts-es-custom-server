{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ESNext",
	  "lib": ["ESNext"],
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "strict": true,
	  "skipLibCheck": true,
	  "resolveJsonModule": true,
	  "outDir": "./dist"
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", ".next"]
  }
  